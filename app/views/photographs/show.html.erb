<p id="notice"><%= notice %></p>

<div class="photograph">
  <%= image_tag @photograph.image_url.to_s %>
</div>

<p>
  <strong>Title:</strong>
  <%= @photograph.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @photograph.description %>
</p>


<%= link_to 'All Photographs', root_path %><br>
<%= link_to 'Edit', edit_photograph_path(@photograph) %><br>
<%= link_to 'Back', photographs_path %><br>
<%= link_to 'Add Tag', new_photograph_tag_hash_path(@photograph) %>
